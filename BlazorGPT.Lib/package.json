  "devDependencies": {
    "material-components-web": "14.0.0",
    "regexp": "1.0.0",
    "sass": "1.39.2",
    "source-map-loader": "4.0.1",
    "terser": "^5.15.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
